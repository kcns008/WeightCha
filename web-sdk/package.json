{
  "name": "weightcha",
  "version": "1.0.1",
  "type": "module",
  "description": "WeightCha - Next-generation human verification using trackpad pressure detection",
  "main": "dist/weightcha.cjs",
  "module": "dist/weightcha.esm.js",
  "types": "dist/index.d.ts",
  "browser": "dist/weightcha.min.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/weightcha.esm.js",
      "require": "./dist/weightcha.cjs",
      "browser": "./dist/weightcha.min.js"
    }
  },
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "captcha",
    "human-verification",
    "weightcha",
    "trackpad",
    "pressure-detection",
    "sdk",
    "javascript",
    "web",
    "privacy",
    "accessibility",
    "security",
    "bot-detection"
  ],
  "author": "WeightCha Team <contact@weightcha.com>",
  "license": "MIT",
  "homepage": "https://weightcha.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/weightcha/weightcha.git",
    "directory": "web-sdk"
  },
  "bugs": {
    "url": "https://github.com/weightcha/weightcha/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.2",
    "rollup": "^3.26.3",
    "tslib": "^2.8.1",
    "typescript": "^5.1.6"
  },
  "jsdelivr": "dist/weightcha.min.js",
  "unpkg": "dist/weightcha.min.js"
}
